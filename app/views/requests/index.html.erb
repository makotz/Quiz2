<h1>Support Requests<h1>
<h6>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Department</th>
    <th>Message</th>
    <th>Actions</th>
  </tr>
  <tr>
  <% @requests.each do |request| %>
  <th><%= request.name %></th>
  <th><%= request.email %></th>
  <th><%= request.department %></th>
  <th><%= request.message %></th>
  <th><%= link_to "Edit", edit_request_path(request) %> <%= link_to "Delete",request_path(request), method: :delete, data: {confirm: "Are you sure you want to delete this post?"}%> <%= link_to "#{request.completion}", completion_path(request) %> </th>
  </tr>
  <% end %>
</table>
<%= will_paginate @requests %>
</h6>
